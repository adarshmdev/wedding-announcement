        .wishes-carousel-container {
            position: relative;
            width: 100%;
            overflow: hidden;
        }
        
        .wishes-track {
            display: flex;
            gap: 20px;
            transition: transform 0.5s ease;
            width: max-content;
        }
        
        .wish-slide {
            flex: 0 0 auto;
            width: calc(100vw - 40px);
            max-width: 350px;
        }
        
        .wish-card {
            background-color: white;
            border-radius: 10px;
            padding: 25px;
            box-shadow: 0 4px 15px rgba(0,0,0,0.08);
            min-height: 250px;
            display: flex;
            flex-direction: column;
            justify-content: space-between;
        }
        
        .wish-content p {
            color: #333;
            font-style: italic;
            margin-bottom: 15px;
        }
        
        .wish-author {
            text-align: right;
            font-weight: 600;
            color: #666;
        }
        
        .btn-primary {
            background-color: #d3a625;
            border-color: #d3a625;
            padding: 12px 30px;
            transition: all 0.3s ease;
        }
        
        .btn-primary:hover {
            background-color: #b88d1f;
            border-color: #b88d1f;
        }
        
        @media (max-width: 768px) {
            .wish-slide {
                width: calc(100vw - 60px);
            }
        }
